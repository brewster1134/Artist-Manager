- content_for :js do
  :coffee
    events_path = "#{ events_path }"
    login_path = "#{ login_path }"
    enable_hotkey = "#{ current_user ? false : true }"

%fieldset#events
  %legend Events
  .accordion
    %h4.center LOADING EVENTS...

%fieldset#tags
  %legend Work
  - if Settings.home_show_tag_view == :accordion
    = render "tags_accordion", :tags => @tags
  - else
    = render "tags_plain", :tags => @tags
  
  
      
